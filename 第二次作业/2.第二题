//输入10个整数，统计其中正整数的个数。
#include <stdio.h>
int main()
{
    int count=0, i=1, num;
    printf("请输入十个数字，用空格隔开，按回车键结束");
   while(i <= 10)
   {
       scanf("%d", &num);
       if (num >= 0)
       {
           count++;              //数字为正整数是计数器加一，其他地方同第一题
       }
       i++;
   }
    
    printf("%d", count);
    return 0;
}
